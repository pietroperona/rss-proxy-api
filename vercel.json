{
  "version": 2,
  "functions": {
    "api-python/*.py": {
      "runtime": "python@3.9"
    }
  },
  "routes": [
    { "src": "/api/discover-py", "dest": "/api-python/discover.py" },
    { "src": "/api/rss-py", "dest": "/api-python/rss.py" },
    { "src": "/api/image-proxy-py", "dest": "/api-python/image.py" }
  ]
}